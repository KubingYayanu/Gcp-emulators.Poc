{
  "Serilog": {
    "SeqServerUrl": "http://localhost:901",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Gcp": {
    "PubSub": {
      "Emulated": true,
      "SubscriberAckDeadline": 60
    }
  },
  "Queues": {
    "Worker": {
      // one-to-one
      "PublisherOneToOne": {
        "ProjectId": "test-project",
        "TopicId": "one-to-one",
        "SubscriptionId": "one-to-one-sub"
      },
      "SubscriberOneToOne": {
        "ProjectId": "test-project",
        "TopicId": "one-to-one",
        "SubscriptionId": "one-to-one-sub",
        "SubscriberAckDeadline": 10
      },
      // one-to-many
      "PublisherOneToMany": {
        "ProjectId": "test-project",
        "TopicId": "one-to-many",
        "SubscriptionId": "one-to-many-sub"
      },
      "SubscriberOneToMany1": {
        "ProjectId": "test-project",
        "TopicId": "one-to-many",
        "SubscriptionId": "one-to-many-sub"
      },
      "SubscriberOneToMany2": {
        "ProjectId": "test-project",
        "TopicId": "one-to-many",
        "SubscriptionId": "one-to-many-sub"
      }
    }
  }
}